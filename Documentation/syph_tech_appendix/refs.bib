author = {Saddler, C A and Wu, Y and Valckenborgh, F and Tanaka, M M},
doi = {10.1016/j.epidem.2013.08.002},
isbn = {1878-0067 (Electronic)
1878-0067 (Linking)},
journal = {Epidemics},
keywords = {*Mutation,Anti-Bacterial Agents/*economics/pharmacology,Antibiotic resistance,Antimicrobial therapy,Australia/epidemiology,Bacteria/drug effects/*genetics,Compensatory mutation,Cost of resistance,Drug Resistance, Bacterial/*genetics,Evolution, Molecular,Genotype,Gonorrhea/drug therapy/*economics/epidemiology/mic,Humans,Mathematical Computing,Mathematical model,Models, Genetic,Stochastic Processes},
