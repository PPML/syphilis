keywords = {r03840},
mendeley-tags = {r03840},
number = {4},
pages = {813--820},
title = {{Impact on sexually transmitted disease spread of increased condom use by young females, 1987-1992.}},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed{\&}id=8550280{\&}retmode=ref{\&}cmd=prlinks},
volume = {24},
